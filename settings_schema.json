  {
    "name": "General Settings",
    "settings": [
      {
        "type": "header",
        "content": "Favicon"
      },
      {
        "type": "checkbox",
        "id": "favicon_enable",
        "label": "Use a custom icon"
      },
      {
        "type": "image",
        "id": "favicon.png",
        "label": "Custom favicon",
        "info": "16 x 16px .png required"
      },
      {
        "type": "checkbox",
        "id": "lazyloading_enable",
        "label": "Enable Lazy Loading",
        "default": true,
        "info": "Lazyloading helps in faster loading of images and overall page performance"
      },
      {
        "type": "text",
        "id": "img_size_product",
        "label": "Image size on product page",
        "default": "900x900",
        "info": "Size given in pixels"
      },
      {
        "type": "text",
        "id": "img_width_grid",
        "label": "Image width on product grid",
        "default": "450",
        "info": "Size given in pixels. Used in conjunction with the next option to calculate the final grid images."
      },
      {
        "type": "select",
        "id": "grid_aspect_ratio",
        "label": "Aspect Ratio on product Grid?",
        "info": "The ratio of product grid width vs height. Check the image [here](https:\/\/www.masstechnologist.com\/tfimages\/aspect-ratio.png) for better understanding",
        "options": [
          {
            "value": "3:2",
            "label": "3:2 - Landscape"
          },
          {
            "value": "4:3",
            "label": "4:3 - Landscape"
          },
          {
            "value": "1:1",
            "label": "1:1 - Square"
          },
          {
            "value": "3:4",
            "label": "3:4 - Portrait"
          },
          {
            "value": "2:3",
            "label": "2:3 - Portrait"
          }
        ],
        "default": "1:1"
      },
      {
        "type": "select",
        "id": "grid_auto_resize",
        "label": "Auto Resize on product Grid?",
        "options": [
          {
            "value": "false",
            "label": "Disable"
          },
          {
            "value": "resize",
            "label": "Resize without Crop"
          },
          {
            "value": "resize-crop",
            "label": "Resize with Strech\/crop"
          }
        ],
        "default": "resize-crop",
        "info": "1:Keeps the images in its original size and aspect ratio. 2:Resize the full images to fit. 3:Crop\/Cut images to fit. A portion of mismatched image might be shown but feels better looking."
      },
      {
        "type": "range",
        "id": "grid_layout",
        "label": "Items\/Objects\/Grids Roundness",
        "min": 0,
        "max": 35,
        "step": 1,
        "default": 2,
        "info": "Moving towards zero(0) will make it square and moving away will make them round"
      },
      {
        "type": "select",
        "id": "grid_color_swatches",
        "label": "Color Swatch on product Grid?",
        "options": [
          {
            "value": "false",
            "label": "Disable"
          },
          {
            "value": "color",
            "label": "Color Swatch"
          },
          {
            "value": "thumb",
            "label": "Product Thumb Swatch"
          }
        ],
        "default": "color",
        "info": "Product page display handled from product page settings area"
      },
      {
        "type": "checkbox",
        "id": "limit_title_words",
        "label": "Limit title length",
        "info": "Used to show a part of the title on product grids",
        "default": false
      },
      {
        "type": "text",
        "id": "limit_title_words_count",
        "label": "No of characters to limit Title to",
        "default": "50"
      },
      {
        "type": "text",
        "id": "product_new_time",
        "label": "Days to list products as New",
        "default": "7",
        "info": "Used for showing New label"
      },
      {
        "type": "checkbox",
        "id": "product_grid_img_carousel",
        "label": "Slide All images in product grid?",
        "default": true,
        "info": "Used for Product grid carousel enable\/disable"
      },
      {
        "type": "checkbox",
        "id": "hide_hover",
        "label": "Hide hover images on product grid",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "product_show_compare_at_price",
        "label": "Show compare at price",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "product_reviews_enable",
        "label": "Enable product reviews",
        "info": "Install the free Shopify [Product Reviews](https:\/\/apps.shopify.com\/product-reviews) app to add product reviews."
      },
      {
        "type": "checkbox",
        "id": "grid_vendor_enable",
        "label": "Show brand names on product grid",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "hide_deal",
        "label": "Hide deal info on product grid",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "hide_wishlist",
        "label": "Hide wishlist option",
        "default": false
      },
